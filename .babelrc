{
  "presets": ["@babel/typescript", "@babel/react"],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-syntax-dynamic-import"
  ],
  "env": {
    "development": {
      "presets": [
        [
          "@babel/env",
          {
            "modules": false,
            "useBuiltIns": "entry",
            "corejs": 2
          }
        ]
      ]
    }
  }
}
